<Window x:Class="OneView_MappingSet_MVVM.UI.View.MainWindowView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OneView_MappingSet_MVVM.UI.View"
        mc:Ignorable="d"
        Title="Mapping set generator" 
        Height="400" Width="600" MinHeight="400" MinWidth="600" Background="{StaticResource WindowBackgroundColor}">
    <Grid>
        <DockPanel Grid.ColumnSpan="2">
            <Frame x:Name="WindowContent"/>
        </DockPanel>
    </Grid>
</Window>
